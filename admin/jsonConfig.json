{
	"i18n": true,
	"type": "panel",
	"items": {
		"_userdata": {
			"newLine": true,
			"hidden": "true",
			"type": "header",
			"text": "User Data",
			"sm": 12,
			"md": 12,
			"lg": 12,
			"size": 3
		},
		"userName": {
			"type": "text",
			"label": "user name",
			"newLine": true
		},
		"userPassword": {
			"type": "text",
			"label": "user password",
			"newLine": true
		},
		"_connection": {
			"newLine": true,
			"type": "header",
			"text": "Connection settings",
			"sm": 12,
			"md": 12,
			"lg": 12,
			"size": 3
		},
		"mqttBroker": {
			"type": "text",
			"label": "mqtt broker",
			"newLine": true
		},
		"mqttPort": {
			"type": "text",
			"label": "mqtt port",
			"newLine": false
		},
		"_auth": {
			"newLine": true,
			"type": "header",
			"text": "MQTT Authentication settings",
			"sm": 12,
			"md": 12,
			"lg": 12,
			"size": 3
		},
		"mqttUserId": {
			"type": "text",
			"label": "mqtt userId",
			"newLine": false
		},
		"mqttUserName": {
			"type": "text",
			"label": "mqtt user name",
			"newLine": false
		},
		"mqttPwd": {
			"type": "text",
			"label": "mqtt pwd",
			"newLine": true
		},
		"mqttClientId": {
			"type": "text",
			"label": "mqttClientId",
			"newLine": false
		},
		"_create": {
			"type": "sendTo",
			"label": "Create Login Data",
			"command": "create",
			"variant": "outlined",
			"useNative": true,
			"jsonData": "{\"user\": \"${data.mqttUserName}\", \"pass\": \"${data.mqttPwd}\"}}"
		},
		"_test": {
			"type": "sendTo",
			"label": "Test Message Box Feedback",
			"command": "test",
			"variant": "contained",
			"showProcess": true,
			"jsonData": "{\"user\": \"${data.mqttUserName}\", \"pass\": \"${data.mqttPwd}\", \"url\": \"${data.mqttBrokerl}\", \"port\": ${data.mqttPort}, \"clientId\": \"${data.mqttClientId}\"}"
		},
		"_send": {
			"type": "sendTo",
			"label": "Send for MSG",
			"command": "send",
			"variant": "outlined",
			"showProcess": true,
			"jsonData": "{\"user\": \"${data.userName}\", \"msg\": \"Hello World\"}}"
		},
		"_device": {
			"newLine": true,
			"type": "header",
			"text": "Device settings",
			"sm": 12,
			"md": 12,
			"lg": 12,
			"size": 3
		},
		"deviceName": {
			"type": "text",
			"label": "device Name",
			"newLine": true
		},
		"deviceId": {
			"type": "text",
			"label": "device Id",
			"newLine": true
		},
		"_devicetable": {
			"newLine": true,
			"type": "header",
			"text": "Device settings in table format",
			"sm": 12,
			"md": 12,
			"lg": 12,
			"size": 3
		},
		"devices": {
			"sm": 12,
			"newLine": true,
			"type": "table",
			"items": [
				{
					"type": "text",
					"width": "20%",
					"title": "name",
					"attr": "devname",
					"filter": false,
					"sort": false,
					"default": ""
				},
				{
					"type": "text",
					"width": "20%",
					"title": "id",
					"attr": "devid",
					"filter": false,
					"sort": false,
					"default": ""
				},
				{
					"type": "select",
					"width": "20%",
					"options": [
						{ "value": "none", "label": "none" },
						{ "value": "basic22", "label": "Version 2,2kWh" }
					],
					"title": "Battery Type",
					"attr": "streamType",
					"filter": false,
					"sort": false,
					"default": "none"
				}
			]
		}
	}
}
